{
  "name": "@quantumart/qp8-widget-platform-shell",
  "version": "8.0.14",
  "description": "Данный проект является частью системы QP8.WidgetPlatform и содержит инфраструктурный шаблон (оболочку) для загрузки модулей",
  "author": "Quantum Art (http://quantumart.ru/) Alferov Vitalii",
  "license": "Apache License Version 2.0",
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@loadable/webpack-plugin": "^5.15.2",
    "@types/compression": "^1.7.2",
    "@types/loadable__server": "^5.12.6",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "css-loader": "^6.7.3",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "5.5.1",
    "postcss-loader": "^7.2.4",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^5.0.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "serve": "14.2.0",
    "style-loader": "^3.3.2",
    "typescript": "^5.2.2",
    "webpack": "5.80.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "npm run build:local && webpack-cli serve --config config/webpack.config.client.standalone.dev._.js",
    "build": "rimraf dist && npm run build:client:prod",
    "start:local": "npm run build:local && npm run serve",
    "build:local": "rimraf dist && npm run build:client:local",
    "build:client:local": "webpack --mode production --progress --config config/webpack.config.client.standalone.local._.js",
    "build:client:prod": "webpack --mode production --progress --config config/webpack.config.client.standalone.prod._.js",
    "serve": "serve dist -p 3200",
    "start:server": "npm run build:server && npm run node:serve",
    "build:server": "rimraf dist && npm run build:server:client && npm run build:server:server",
    "build:server:client": "webpack --mode development --env nodeserver --progress --config config/webpack.config.client.server.local._.js",
    "build:server:server": "webpack --mode development --env nodeserver --progress --config config/webpack.config.server._.js",
    "build:server:prod": "rimraf dist && npm run build:server:client:prod && npm run build:server:server:prod",
    "build:server:client:prod": "webpack --mode production --env nodeserver --env prodstate --progress --config config/webpack.config.client.server.prod._.js",
    "build:server:server:prod": "webpack --mode production --env nodeserver --env prodstate --progress --config config/webpack.config.server._.js",
    "node:serve": "node ./dist/server/main.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@loadable/server": "^5.15.3",
    "@module-federation/node": "^0.14.0",
    "@module-federation/typescript": "^2.3.0",
    "@quantumart/qp8-widget-platform-bridge": "^8.0.10",
    "@quantumart/qp8-widget-platform-shell-core": "^8.0.10",
    "autoprefixer": "^10.4.14",
    "buffer": "^6.0.3",
    "compression": "^1.7.4",
    "dangerously-set-html-content": "^1.0.13",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "js-event-bus": "^1.1.1",
    "node-cache": "^5.1.2",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.10.0",
    "source-map-loader": "^4.0.1",
    "stream-browserify": "^3.0.0"
  },
  "keywords": [
    "npm",
    "package",
    "react",
    "QP",
    "QP8",
    "QP8.WidgetPlatform"
  ]
}
